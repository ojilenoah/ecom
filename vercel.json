{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/$1"
    }
  ]
}